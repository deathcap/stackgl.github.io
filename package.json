    "Chris Dickinson <chris@neversaw.us>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gl-modules/gl-modules.github.io/issues"
  },
  "homepage": "https://github.com/gl-modules/gl-modules.github.io",
  "devDependencies": {
    "beefy": "^2.0.3"
  }
}
