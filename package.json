{
  "name": "stack.gl",
  "version": "0.0.0",
  "description": "stack.gl's site",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "brfs": "^1.1.2",
    "cheerio": "^0.17.0",
    "district": "^1.0.1",
    "domify": "^1.3.0",
    "findup-element": "0.0.0",
    "frame-debounce": "0.0.0",
    "markdown-tree": "0.0.0",
    "marked": "^0.3.2",
    "minstache": "^1.2.0",
    "scoped-bulk": "^1.0.0",
    "sliced": "0.0.5",
    "unhtml": "^0.1.0"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "bundledDependencies": [
    "splash-grid"
  ],
  "scripts": {
    "prestart": "npm run links && node data/regenerate",
    "start": "beefy index.js:bundle.js --open",
    "postinstall": "npm run links && scoped-bulk stackgl npm install && npm dedupe",
    "bundle": "npm run prestart && browserify index.js -o bundle.js",
    "links": "district stackgl splash-grid splash-flock"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gl-modules/gl-modules.github.io.git"
  },
